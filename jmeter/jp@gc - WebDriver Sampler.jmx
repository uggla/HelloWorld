def what = WDS.browser.findElement(By.id(&apos;text-input-what&apos;))
what.clear()
what.sendKeys(&apos;Selenium&apos;)
def where = WDS.browser.findElement(By.id(&apos;text-input-where&apos;))
where.clear()
where.sendKeys(&apos;india&apos;)

def screenshot = WDS.browser.getScreenshotAs(OutputType.FILE)
FileUtils.copyFile(screenshot, new File(&apos;c:/temp/messages1.png&apos;))

WDS.sampleResult.sampleEnd();
