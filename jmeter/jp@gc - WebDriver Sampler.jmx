var fileUtils = JavaImporter(org.apache.commons.io)
var io = JavaImporter(java.io)

var what = WDS.browser.findElement(selenium.By.id(&apos;text-input-what&apos;))
what.clear()
what.sendKeys([&apos;Selenium&apos;])
var where = WDS.browser.findElement(selenium.By.id(&apos;text-input-where&apos;))
where.clear()
where.sendKeys([&apos;india&apos;])
var screenshot = WDS.browser.getScreenshotAs(selenium.OutputType.FILE)
//fileUtils.FileUtils.copy(screenshot, new io.File(&apos;C:\temp\screenshot.png&apos;))
//screenshot.renameTo(io.File(&apos;c:\temp\screenshot.png&apos;))
fileUtils.FileUtils.copyFile(screenshot, new io.File(&apos;c:/temp/messages1.png&apos;))


